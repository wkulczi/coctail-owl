###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#AlcoholFreeDrinks
:AlcoholFreeDrinks rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Drink
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasStrength ;
                                                                owl:someValuesFrom :AlcoholFree
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Drink .


