<http://semantic.cs.put.poznan.pl/ontologie/cocktails> rdf:type owl:Ontology ;
                                                        owl:versionIRI <http://semantic.cs.put.poznan.pl/ontologie/cocktails/1.0.0> ;
                                                        owl:versionInfo "Pierwsza wersja zestawu danych - schemat, relacje i trochę elementów"@pl .

#################################################################
#    Object Properties
#################################################################

###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#canBeReplacedWith
:canBeReplacedWith rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf :canReplace ;
                   rdf:type owl:TransitiveProperty ;
                   rdfs:domain :Ingredient ;
                   rdfs:range :Ingredient .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#canReplace
:canReplace rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain :Ingredient ;
            rdfs:range :Ingredient .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasAlcohol
:hasAlcohol rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            owl:inverseOf :isAlcoholFor .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasCountryOfOrigin
:hasCountryOfOrigin rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:range [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCountryOfOrigin ;
                                 owl:someValuesFrom :OriginCountry
                               ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasDifficulty
:hasDifficulty rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:range [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDifficulty ;
                            owl:someValuesFrom :Difficulty
                          ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasFruit
:hasFruit rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasIngredient ;
          owl:inverseOf :isFruitFor .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasGarnish
:hasGarnish rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            owl:inverseOf :isGarnishFor ;
            rdfs:domain [ rdf:type owl:Restriction ;
                          owl:onProperty :hasGarnish ;
                          owl:someValuesFrom :Drink
                        ] ;
            rdfs:range [ rdf:type owl:Restriction ;
                         owl:onProperty :hasGarnish ;
                         owl:someValuesFrom :Garnish
                       ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasGlass
:hasGlass rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasIngredient ;
          owl:inverseOf :isGlassFor ;
          rdfs:domain [ rdf:type owl:Restriction ;
                        owl:onProperty :hasGlass ;
                        owl:someValuesFrom :Drink
                      ] ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :hasGlass ;
                       owl:someValuesFrom :Glass
                     ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasHerb
:hasHerb rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasIngredient ;
         owl:inverseOf :isHerbFor .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasNonAlcoholic
:hasNonAlcoholic rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasIngredient ;
                 owl:inverseOf :isNonAlcoholicFor .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasOtherIngredient
:hasOtherIngredient rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasIngredient ;
                    owl:inverseOf :isOtherIngredientFor .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasState
:hasState rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasStrength
:hasStrength rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain [ rdf:type owl:Restriction ;
                           owl:onProperty :hasStrength ;
                           owl:someValuesFrom :Drink
                         ] ;
             rdfs:range [ rdf:type owl:Restriction ;
                          owl:onProperty :hasTheme ;
                          owl:someValuesFrom :Strength
                        ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasTemp
:hasTemp rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:FunctionalProperty .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#hasTheme
:hasTheme rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :hasTheme ;
                       owl:someValuesFrom :Theme
                     ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isAlcoholFor
:isAlcoholFor rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isFruitFor
:isFruitFor rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isGarnishFor
:isGarnishFor rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isIngredientOf ;
              rdfs:domain [ rdf:type owl:Restriction ;
                            owl:onProperty :isGarnishFor ;
                            owl:someValuesFrom :Garnish
                          ] ;
              rdfs:range [ rdf:type owl:Restriction ;
                           owl:onProperty :isGlassFor ;
                           owl:someValuesFrom :Drink
                         ] .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isGlassFor
:isGlassFor rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isHerbFor
:isHerbFor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isIngredientOf
:isIngredientOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isMadeBy
:isMadeBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isNonAlcoholicFor
:isNonAlcoholicFor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :isIngredientOf .


###  http://semantic.cs.put.poznan.pl/ontologie/cocktails#isOtherIngredientFor
:isOtherIngredientFor rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :isIngredientOf .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain :Ingredient ;
                      rdfs:range :Ingredient .


